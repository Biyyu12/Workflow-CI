name: Loan-Status-Prediction

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      learning_rate: {type: float, default: 0.1}
      max_depth: {type: int, default: 5}
      dataset: {type: path, default: "loan_data_preprocessed.csv"}

    command: >
      python modelling.py
      {n_estimators}
      {learning_rate}
      {max_depth}
      {dataset}